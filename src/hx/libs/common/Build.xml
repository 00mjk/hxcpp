<xml>

<pragma once="true" />


<files id="hxcpp_lib_common" >
  <depend files="hxcpp-depends"/>
  <depend name="${this_dir}/Build.xml" dateOnly="true" />
  <depend name="sha1.h" />
  <depend name="socket.h" />
  <cache value="true" />

  <file name="socket.cpp"/>
  <file name="sha1.cpp"/>
</files>

<target id="haxe">
  <files id="hxcpp_lib_common"/>
  <lib name="ws2_32.lib" if="windows" unless="static_link" />
</target>


</xml>
